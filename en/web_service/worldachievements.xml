    <!-- ~~~ 17 - Igloo Party ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ --> 
	
	<achievement name="Igloo Party" id="17"> 
		<event>user enterIgloo</event> 
